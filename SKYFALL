local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Skyfall Script Hubs",
   LoadingTitle = "Preparing your AMAZING script hub😎",
   LoadingSubtitle = "by Kotokha",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "Player Changer"
   },
   Discord = {
      Enabled = true,
      Invite = "TA7TfCSVCQ",
      RememberJoins = false
   },
   KeySystem = false,
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"}
   }
})

Rayfield:Notify({
   Title = "Executed The Script Successfully",
   Content = "A Good Script Hub",
   Duration = 5,
   Actions = {
      Ignore = {
         Name = "Okay!",
         Callback = function()
            print("The user tapped Okay!")
         end
      }
   }
})

local UniversalsTab = Window:CreateTab("Universals", nil)
UniversalsTab:CreateSection("Universal Scripts")

UniversalsTab:CreateButton({
   Name = "Infinite Yield",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
   end
})

