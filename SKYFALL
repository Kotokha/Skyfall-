-- Fondo de Gojo
local player = game.Players.LocalPlayer
local gui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
gui.Name = "SkyfallBackground"
gui.ResetOnSpawn = false

local background = Instance.new("ImageLabel", gui)
background.Name = "BackgroundImage"
background.Image = "rbxassetid://11401525648" -- ID de imagen de Gojo
background.Size = UDim2.new(1, 0, 1, 0)
background.Position = UDim2.new(0, 0, 0, 0)
background.BackgroundTransparency = 1
background.ImageTransparency = 0.2 -- Ajusta esto para que el menú se vea mejor encima
background.ZIndex = 0

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Skyfall Script Hub",
   LoadingTitle = "Preparing your AMAZING script hub😎",
   LoadingSubtitle = "made by Kotokha",
   ConfigurationSaving = {
      Enabled = true,n 
      FolderName = nil,
      FileName = "Player Changer"
   },
   Discord = {
      Enabled = true,
      Invite = "TA7TfCSVCQ",
      RememberJoins = false,
   },
   KeySystem = false,
   KeySettings = {
      Title = "Untitled",
      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Hello"}
   }
})

Rayfield:Notify({
   Title = "Executed The Script Successfully",
   Content = "A Good Script Hub",
   Duration = 5,
   Image = nil,
   Actions = {
      Ignore = {
         Name = "Okay!",
         Callback = function()
            print("The user tapped Okay!")
         end
      },
   },
})

-- Universals Tab
local UniversalsTab = Window:CreateTab("Universals", nil)
local UniversalsSection = UniversalsTab:CreateSection("Universals scripts")

